<div class="row">
  <aside class="col-sm-4">
    <div class="card">
      <div class="card-header">
        <h5 class="card-title"><%= @itinerary.title %></h5>
      </div>
      <div class="card-body">
        <img class="rounded img-fluid" src="" alt="">
        <p><%= @itinerary.user.name %></p>
        <%= @itinerary.start_date %>～<%= @itinerary.end_date %>
      </div>
    </div>
    <div>
      <%= link_to '編集する', edit_itinerary_path, class:'btn btn-primary btn-block' %>
      <%= link_to '削除する', @itinerary, method: :delete, data: { confirm: '本当に削除してよろしいですか？' }, class:'btn btn-primary btn-block' %>
    </div>
  </aside>
  <div class="col-sm-8">
    <%= @itinerary.start_date %>～<%= @itinerary.end_date %>
    <p>ここにscheduleを入れる。</p>
  </div>
</div>